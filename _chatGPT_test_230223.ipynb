    "def conv_rate(ori_l):\n",
    "    res = ori_l\n",
    "    if sum(ori_l) != 100:\n",
    "        for i in range(0,len(ori_l)-1):\n",
    "            res[i] = round(ori_l[i] * 100 / sum(ori_l),2)\n",
    "        res[len(ori_l)-1] = round(100 - sum(res[:-1]),2)\n",
    "    return res"
   ]
